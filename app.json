{
  "expo": {
    "name": "Dualtemp Weather",
    "slug": "dualtemp-weather",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1B4D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "supportsRTL": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow DualTemp Weather to use your location."
        }
      ],
      [
        "expo-localization"
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show you the weather."
      },
      "bundleIdentifier": "com.ekarni.rndualtempweatherapp",
      "buildNumber": "1.2.2"
    },
    "android": {
      "versionCode": 12,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1C1B4D"
      },
      "package": "com.ekarni.rndualtempweatherapp"
    },
    "updates": {
      "url": "https://u.expo.dev/444bda66-1ab4-4665-ba53-c2b76743a33b"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}