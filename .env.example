# Application Configuration
# Copy this file to .env.local and update with your actual values
# DO NOT commit .env.local to version control

# Support email for users to contact when experiencing persistent issues
EXPO_PUBLIC_SUPPORT_EMAIL=support@example.com

# Splash screen timeout in milliseconds
# After this time, the app will show UI regardless of loading state
EXPO_PUBLIC_SPLASH_TIMEOUT_MS=3000

# Sentry Configuration
# Get your DSN from https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
# Leave empty to disable Sentry crash reporting
EXPO_PUBLIC_SENTRY_DSN=

#DO NOT COMMIT THIS
EXPO_PUBLIC_SENTRY_AUTH_TOKEN=
