# Application Configuration
# Copy this file to .env.local and update with your actual values
# DO NOT commit .env.local to version control

# Support email for users to contact when experiencing persistent issues
EXPO_PUBLIC_SUPPORT_EMAIL=support@example.com

# Splash screen timeout in milliseconds
# After this time, the app will show UI regardless of loading state
EXPO_PUBLIC_SPLASH_TIMEOUT_MS=3000

# Sentry Configuration
# Get your DSN from https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
# Leave empty to disable Sentry crash reporting
EXPO_PUBLIC_SENTRY_DSN=

# Sentry Auth Token (build-time only, DO NOT commit)
# Used for uploading source maps during builds
# Get from: https://sentry.io/settings/account/api/auth-tokens/
# IMPORTANT: Do NOT use EXPO_PUBLIC_ prefix - this token must never be exposed to clients
SENTRY_AUTH_TOKEN=
